    "    Tudo_certo=true\n",
    "\n",
    "    # Uma dinâmica aplicada no vetor nulo sempre dará como resposta o vetor nulo.\n",
    "    for n=2:k\n",
    "        A=randn(n,n)                # Matriz A qualquer\n",
    "        x0=zeros(n,1)               # Dado inicial nulo\n",
    "        for i=1:k\n",
