    "function teste_qr(k)\n",
    "    Tudo_certo=true\n",
    "    for n=1:k\n",
    "        for m=1:k\n",
    "            A=randn(n,m)\n",
    "            Q,R=decomposição_qr(A)\n",
    "            if norm(A-Q*R)>0.00001\n",
    "                if Q*Q'!=I\n",
    "                    Tudo_certo=false\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "    end\n",
    "        \n",
    "    return Tudo_certo\n",
    "end"
